<XLangExampleProgram>
    <!--A sample XML language program-->
    <var name="myVar1" value="10">
        <value desc="This is an overridden value">
            <attr1 field1="123" field2="A123"/>
        </value>
    </var>
    <var name="mySimpleVar" value="True"/>
    <func>
        <id>101</id>
        <firstname>Naman</firstname>
        <lastname>Kumar</lastname>
        <subject>Math</subject>
        <boolFlag>False</boolFlag>
        <marks attrWontAppear="true">83</marks>
        <obj attr1="abc" attr2="100.3" attr3="10">
            <![CDATA[
            id: 10
            name: myObj
            ]]>
            anotherTextBasedField: Value
        </obj>
    </func>
</XLangExampleProgram>